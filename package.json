    "url": "git@github.com:axentix/neumorphic.git"
  },
  "bugs": {
    "url": "https://github.com/axentix/neumorphic/issues"
  },
  "style": "./dist/neu-axentix.min.css",
  "sass": "./src/neu-axentix.scss",
  "main": "dist/neu-axentix.min.js",
